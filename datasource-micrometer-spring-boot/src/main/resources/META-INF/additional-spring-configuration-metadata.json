{
  "groups": [
    {
      "name": "jdbc",
      "description": "Namespace for controlling CMF auto-configurations."
    }
  ],
  "properties": [
    {
      "name": "jdbc.datasource-proxy.enabled",
      "type": "java.lang.Boolean",
      "description": "Whether to enable JDBC instrumentation.",
      "defaultValue": true,
      "sourceType": "net.ttddyy.observation.boot.autoconfigure.DataSourceObservationAutoConfiguration"
    },
    {
      "name": "jdbc.opentelemetry.enabled",
      "type": "java.lang.Boolean",
      "description": "Whether to enable OpenTelemetry Semantic Conventions support.",
      "defaultValue": true,
      "sourceType": "net.ttddyy.observation.boot.autoconfigure.opentelemetry.DataSourceObservationOpenTelemetryAutoConfiguration"
    }
  ]
}
